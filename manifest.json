{
  "manifest_version": 3,
  "name": "YT_Focus",
  "version": "1.0",
  "description": "Focus Mode for Youtube.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
